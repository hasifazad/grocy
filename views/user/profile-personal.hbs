<div class="container">
    <div class="row">
        {{>user-profile-menu}}
        <div class="col-6">
            <form id="address_form" action="/save-address" method="post" class="row g-3 p-5 mt-4 border"
                onsubmit="saveAddress()">
                <div class="col-12">
                    <label for="" class="form-label">Full Name</label>
                    <input type="text" class="form-control" id="" name="" value="{{userData.username}}" disabled>
                </div>
                <div class="col-md-6">
                    <label for="" class="form-label">Email</label>
                    <input type="text" class="form-control" id="" name="" value="{{userData.emailid}}" disabled>
                </div>
                <div class="col-md-6">
                    <label for="" class="form-label">Mobile</label>
                    <input type="text" class="form-control" id="" name="" value="{{userData.mobile}}" disabled>
                </div>
            </form>
        </div>
    </div>
</div>